#include <iostream>
#include "opencv2/cudafeatures2d.hpp"
#include <opencv/cv.h>
#include <opencv2/highgui/highgui.hpp>
#include <opencv2/core/core.hpp>

using namespace std;
using namespace cv;

int main()
{
	Mat testImg = imread( "data/test.jpg" );

	if( !testImg.data )
	{
		cout <<"load data failed" <<endl;
	}

	imshow("test", testImg);
	waitKey();
	cout << "starting" << endl;

	return 0;
}
