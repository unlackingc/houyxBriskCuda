#include <iostream>
#include "opencv2/cudafeatures2d.hpp"
#include <opencv/cv.h>
#include <opencv2/highgui/highgui.hpp>
#include <opencv2/core/core.hpp>

using namespace std;
using namespace cv;
using namespace cv::cuda;

int main()
{
	Mat testImg = imread( "data/test.jpg" );
	Mat testImgGray;
	cv::cvtColor(testImg, testImgGray, CV_BGR2GRAY);
	if( !testImg.data )
	{
		cout <<"load data failed" <<endl;
	}

	imshow("test", testImgGray);
	waitKey();


	Ptr<cv::cuda::FastFeatureDetector> fastDetector_;
	fastDetector_ = cuda::FastFeatureDetector::create();

	GpuMat fastKpRange;
	InputArray mask;
	fastDetector_->detectAsync(imagePyr_[level], fastKpRange, mask, stream);





	cout << "starting" << endl;

	return 0;
}
